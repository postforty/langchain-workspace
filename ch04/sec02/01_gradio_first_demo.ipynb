    "import random\n",
    "import gradio as gr\n",
    "\n",
    "def random_response(message, history):\n",
    "    return random.choice([\"안녕하세요!\", \"저는 챗봇입니다.\", \"무엇이든 물어보세요~\"])\n",
    "\n",
    "demo = gr.ChatInterface(random_response, type=\"messages\", autofocus=False)\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    demo.launch()\n"
   ]
  }
